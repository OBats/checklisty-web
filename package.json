{
	"name": "check-list",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"react-router-dom": "^4.3.1",
		"react-scripts": "2.1.3"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint-staged": "lint-staged"
	},
	"eslintConfig": {
		"extends": ["eslint:recommended","plugin:react/recommended", "react-app"]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"babel-eslint": "8",
		"eslint": "4.x",
		"eslint-plugin-react": "^7.12.4",
		"husky": "^1.3.1",
		"lint-staged": "^8.1.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.jsx" : [
      "eslint",
      "eslint --fix",
			"git add"
    ],
    "*.js": [
      "eslint",
      "eslint --fix",
			"git add"
    ]
  },
  "pre-commit": "lint-staged"
}
